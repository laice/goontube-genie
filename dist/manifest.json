{
  "manifest_version": 2,

  "name": "Goontube Genie",
  "description": "A framework for interacting with goontube via extensions",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
        "19":"images/icon-19.png",
        "38":"images/icon-38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://www.googleapis.com/"
  ],
  "icons": { "16": "images/icon-16.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"

  },
  "background": {
    "scripts": ["settings.js"]
  }


}